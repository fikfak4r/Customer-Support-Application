<div>
  <div class="form-group">
    <label>Customer Id</label>
    <ul class="ul-dropdown">
      <li class="list-dropdown">
        <input type="text" data-role="combobox" class="form-control" data-bind="value:CustomerIdFld,source:CustomerIddtSrc,events:{change:CustomerIdFldChanged}" data-text-field="" data-value-field="" />
      </li>
      <li>
        <span style="border-left:none" id="CustomerIdInplaceBtn" />
      </li>
    </ul>
  </div>
  <div class="form-group">
    <label>Date</label>
    <input class="form-control" data-role="datepicker" style="width:100%" data-bind="value:DateFld" />
  </div>
  <div class="form-group">
    <label>Discount</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:DiscountFld" />
  </div>
  <div class="form-group hidden">
    <label>Has Transactions Details</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:HasTransactionsDetailsFld" />
  </div>
  <div class="form-group hidden">
    <label>Is Advanced</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsAdvancedFld" />
  </div>
  <div class="form-group hidden">
    <label>Is Fully Paid</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsFullyPaidFld" />
  </div>
  <div class="form-group hidden">
    <label>Is Fully Received</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsFullyReceivedFld" />
  </div>
  <div class="form-group hidden">
    <label>Is In Progress</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsInProgressFld" />
  </div>
  <div class="form-group hidden">
    <label>Is Integer Trailing Order Id With Prefix Po</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsIntegerTrailingOrderIdWithPrefixPoFld" />
  </div>
  <div class="form-group hidden">
    <label>Is Open</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:IsOpenFld" />
  </div>
  <div class="form-group">
    <label>Location Id</label>
    <ul class="ul-dropdown">
      <li class="list-dropdown">
        <input type="text" data-role="combobox" class="form-control" data-bind="value:LocationIdFld,source:LocationIddtSrc,events:{change:LocationIdFldChanged}" data-text-field="" data-value-field="" />
      </li>
      <li>
        <span style="border-left:none" id="LocationIdInplaceBtn" />
      </li>
    </ul>
  </div>
  <div class="form-group">
    <label>Order Id</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:OrderIdFld" />
  </div>
  <div class="form-group hidden">
    <label>Status</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:StatusFld" />
  </div>
  <div class="form-group">
    <label>Subject</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:SubjectFld" />
  </div>
  <div class="form-group">
    <label>Tax</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TaxFld" />
  </div>
  <div class="form-group hidden">
    <label>Ticket Id</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TicketIdFld" />
  </div>
  <div class="form-group hidden">
    <label>Total Amount</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TotalAmountFld" />
  </div>
  <div class="form-group hidden">
    <label>Total Amount Left</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TotalAmountLeftFld" />
  </div>
  <div class="form-group hidden">
    <label>Total Amount Paid</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TotalAmountPaidFld" />
  </div>
  <div class="form-group hidden">
    <label>Transaction Id</label>
    <input type="text" class="form-control k-textbox" style="width:100%" data-bind="value:TransactionIdFld" />
  </div>
</div>